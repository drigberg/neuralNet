{
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "globals": {
      "angular": false,
      "module": true,
      "document": true,
      "console": true,
      "inject": false,
      "Promise": true
    },
    "env": {
      "browser": false,
      "amd": true,
      "node": true,
      "mocha": true,
      "jest": true
    },
    "plugins": [
        "jsdoc"
    ],
    "rules": {
      "no-console": 0,
      "no-undef": 2,
      "no-compare-neg-zero": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-empty": 2,
      "no-duplicate-case": 2,
      "no-ex-assign": 2,
      "no-irregular-whitespace": 2,
      "no-unreachable": 2,
      "valid-typeof": 2,
      "curly": 2,
      "default-case": 2,
      "no-else-return": 2,
      "no-empty-function": 2,
      "no-multi-spaces": 2,
      "no-unexpected-multiline": 2,
      "no-return-await": 2,
      "no-self-assign": 2,
      "no-self-compare": 2,
      "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
      "no-unused-expressions": 0,
      "no-useless-return": 2,
      "require-await": 2,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "prefer-const": 2,
      "prefer-spread": 2,
      "quotes": ["error", "single"],
      "require-jsdoc": ["error", {
        "require": {
          "ClassDeclaration": true,
          "FunctionDeclaration": true,
          "MethodDefinition": true
        }
      }],
      "semi": 2
    }
}
  